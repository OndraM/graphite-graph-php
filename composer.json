    "graphite",
    "graph",
    "builder",
    "url"
  ],
  "homepage": "https://github.com/bd808/graphite-graph-php",
  "type": "library",
  "license": "BSD-2",
  "authors": [
    {
      "name": "Bryan Davis",
      "email": "bd808@bd808.com",
      "homepage": "http://bd808.github.com"
    },
    {
      "name": "Ond≈ôej Machulda",
      "email": "ondrej.machulda@gmail.com"
    }
  ],
  "require": {
    "php": "^5.5 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Graphite\\": "src/Graphite/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Graphite\\": "tests/Graphite/"
    }
  },
  "require-dev": {
